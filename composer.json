{
    "name": "cycle/entity-behavior",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "psr/event-dispatcher": "^1",
        "cycle/orm": "^2.0",
        "cycle/schema-builder": "^2.0",
        "psr/container": "^1.0|^2.0",
        "yiisoft/injector": "^1.0"
    },
    "require-dev": {
        "cycle/annotated": "^3.0",
        "ramsey/uuid": "^4.0",
        "phpunit/phpunit": "^9.5",
        "spiral/tokenizer": "^2.8",
        "vimeo/psalm": "^4.13"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cycle\\ORM\\Entity\\Behavior\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cycle\\ORM\\Entity\\Behavior\\Tests\\": "tests/Behavior"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
